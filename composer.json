{
    "name": "phpcfdi/rfclinc",
    "description": "Listado de RFC Inscritos No Cancelados (php library and application)",
    "keywords": ["sat", "mexico", "lrfc"],
    "homepage": "https://github.com/phpcfdi/rfclinc",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com",
            "homepage": "http://eclipxe.com.mx"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "silex/silex": "~2.2",
        "symfony/console": "~3.4",
        "psr/log": "^1.0",
        "eclipxe/engineworks-progress-status": "^1.0",
        "pimple/pimple": "^3.2",
        "symfony/http-foundation": "^3.4"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "symfony/browser-kit": "^3.4",
        "phpunit/phpunit": "^6.2",
        "overtrue/phplint": "^1.0",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpstan/phpstan-shim": "^0.9.1",
        "maglnet/composer-require-checker": "^0.1.6"
    },
    "autoload": {
        "psr-4": {
            "PhpCfdi\\RfcLinc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCfdi\\RfcLinc\\Tests\\": "tests/"
        }
    }
}
